dataset.trv;frequencyVal=popup.dataset.frv;mrid=popup.dataset.mrid;expires=parseInt(popup.dataset.ex);cookie="mspop_"+mrid+"_"+frequency;preparePopup(trigger,frequency,frequencyVal,cookie,expires,popup);prepareCloses(mrid,type,frequency,frequencyVal,cookie,expires,popup);});});function preparePopup(trigger,frequency,frequencyVal,cookie,expires,popup){cookieSSS=cookie+"_sss";if(!Cookies.get(cookie)){switch(frequency){case 0:case 1:if(expires!=0){Cookies.set(cookie,true,{expires:expires});}else{Cookies.set(cookie,true);}
fireTrigger(trigger,popup,cookie);break;case 2:case 3:if(expires!=0){Cookies.set(cookie,1,{expires:expires});}else{Cookies.set(cookie,1);}
Cookies.set(cookie+"_sss",true);if(frequencyVal===1){fireTrigger(trigger,popup,cookie);}
break;case 4:if(expires!=0){Cookies.set(cookie,true,{expires:expires});}else{Cookies.set(cookie,true);}
Cookies.set(cookie+"_sss",true);break;}}else{switch(frequency){case 0:if(expires!=0){JSON.parse(Cookies.get(cookie))?Cookies.set(cookie,false,{expires:expires}):"";}else{Cookies.set(cookie,false);}
break;case 1:JSON.parse(Cookies.get(cookie))?fireTrigger(trigger,popup,cookie):"";break;case 2:cookieVal=parseInt(Cookies.get(cookie));if(!Cookies.get(cookieSSS)&&cookieVal!=-1){Cookies.set(cookieSSS,true);cookieVal++;if(expires!=0){Cookies.set(cookie,cookieVal,{expires:expires});}else{Cookies.set(cookie,cookieVal);}
if(parseInt(frequencyVal)===cookieVal){fireTrigger(trigger,popup,cookie);}}
break;case 3:cookieVal=parseInt(Cookies.get(cookie));if(!Cookies.get(cookieSSS)&&cookieVal!=-1){Cookies.set(cookieSSS,true);cookieVal++;if(expires!=0){Cookies.set(cookie,cookieVal,{expires:expires});}else{Cookies.set(cookie,cookieVal);}
if(cookieVal%parseInt(frequencyVal)===0){fireTrigger(trigger,popup,cookie);}}
break;case 4:if(!Cookies.get(cookieSSS)&&JSON.parse(Cookies.get(cookie))){Cookies.set(cookieSSS,true);fireTrigger(trigger,popup,cookie);}
break;}}}
function fireTrigger(trigger,popup,cookie){switch(trigger){case 0:addActive(popup);break;case 1:setTimeout(function(){addActive(popup);},triggerVal*1000);break;case 2:document.addEventListener("mouseleave",function(){addActive(popup);});break;case 3:scrollUpCheck(popup,cookie);break;case 4:scrollDownCheck(popup,parseInt(triggerVal));break;case 5:elements=document.querySelectorAll(triggerVal);elements.forEach(function(element){element.addEventListener("click",function(event){event.preventDefault();addActive(popup);});});break;default:addActive(popup);}}
function prepareCloses(mrid,type,frequency,frequencyVal,cookie,expires,popup){btnClose="btnMSPopClose_"+mrid;document.getElementById(btnClose).addEventListener("click",function(){removeActive(popup);closeCookie(frequency,cookie,expires);});if(type===0){divClose="divMSPopOverlay_"+mrid;document.getElementById(divClose).addEventListener("click",function(){removeActive(popup);closeCookie(frequency,cookie,expires);});document.onkeydown=function(e){e=e||window.event;if(e.keyCode===27){removeActive(popup);closeCookie(frequency,cookie,expires);}};}}
function closeCookie(frequency,cookie,expires){switch(frequency){case 0:case 1:case 4:expires!=0?Cookies.set(cookie,false,{expires:expires}):Cookies.set(cookie,false);break;case 2:case 3:expires!=0?Cookies.set(cookie,-1,{expires:expires}):Cookies.set(cookie,-1);break;}}
function toggleActive(popup){popup.classList.toggle(popupActiveClass);}
function removeActive(popup){popup.classList.remove(popupActiveClass);}
function addActive(popup){popup.classList.add(popupActiveClass);}
function destroyPopup(popup){setTimeout(function(){popup.parentNode.removeChild(popup);},1000);}
function scrollUpCheck(popup,cookie){var prev=window.scrollY;window.addEventListener("scroll",function(){var cookieVal=Cookies.get(cookie);if(cookieVal!="false"){window.scrollY>prev?"":addActive(popup);prev=window.scrollY;}});}
function scrollDownCheck(popup,percent){window.addEventListener("scroll",function(){var h=document.documentElement,b=document.body,st="scrollTop",sh="scrollHeight";pt=((h[st]||b[st])/((h[sh]||b[sh])-h.clientHeight))*100;pt>percent?addActive(popup):"";});}



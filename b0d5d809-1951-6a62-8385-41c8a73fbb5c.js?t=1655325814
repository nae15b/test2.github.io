function siteSearch(value,page,keyboard,controls,param,event){url=page+"?"+param+"="+escape(value);if(keyboard){return((event=event||window.event),(!event.which&&!event.keyCode)||(13!=event.which&&13!=event.keyCode)?void 0:(document.getElementById(controls[0]).classList.add(controls[1]),(location.href=url),!1));}else{document.getElementById(controls[0]).classList.add(controls[1]);return(location.href=url),!1;}}
function toggleNav(open){if(open){navOpen.classList.add(naviconClassHide);navClose.classList.remove(naviconClassHide);navContain.classList.add("header__nav-contain--active");}else{navOpen.classList.remove(naviconClassHide);navClose.classList.add(naviconClassHide);navContain.classList.remove("header__nav-contain--active");}}
function closeNav(click){if((!event.target.matches(".header__nav *")&&click)||(event.keyCode===27&&!click)){toggleNav(false);navBtns.forEach((btn)=>{btn.classList.remove(navBtnClassActive);});navNavs.forEach((nav)=>{nav.classList.remove(navNavClassActive);});}}
function wrap(child,tag,classList){if(!tag)tag="div";const parent=child.parentElement;const elm=document.createElement(""+tag);elm.classList=classList;elm.appendChild(child);parent.appendChild(elm);return elm;}

